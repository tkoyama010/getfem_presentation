\begin_layout Frame
import numpy as np 
\end_layout

\begin_layout Frame
from scipy import io 
\end_layout

\begin_layout Frame
from scipy.sparse import linalg
\end_layout

\begin_layout Frame
\begin_inset Flex Alert
status open

\begin_layout Plain Layout
# 質量マトリックスをHarwell Boeing形式でインポート
\end_layout

\end_inset


\end_layout

\begin_layout Frame
A = io.hb_read('MM.hb') 
\end_layout

\begin_layout Frame
\begin_inset Flex Alert
status open

\begin_layout Plain Layout
# 剛性マトリックスをHarwell Boeing形式でインポート
\end_layout

\end_inset


\end_layout

\begin_layout Frame
B = io.hb_read('KK.hb') 
\end_layout

\begin_layout Frame
\begin_inset Flex Alert
status open

\begin_layout Plain Layout
# 固有値解析
\end_layout

\end_inset


\end_layout

\begin_layout Frame
w, v = linalg.eigs(A,M=B)
\end_layout

\begin_layout Frame
omega = np.sqrt(1.000e+00/w)/2.000e+00/np.pi 
\end_layout

\begin_layout Frame
\begin_inset Flex Alert
status open

\begin_layout Plain Layout
# 固有円振動数の保存
\end_layout

\end_inset


\end_layout

\begin_layout Frame
np.savetxt("omega.txt", np.real(omega))
\end_layout

\begin_layout Frame
\begin_inset Flex Alert
status open

\begin_layout Plain Layout
# 固有モードの保存
\end_layout

\end_inset


\end_layout

\begin_layout Frame
np.savetxt("v.txt", np.real(v))
\end_layout

